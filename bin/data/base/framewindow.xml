<?xml version="1.0" ?>
<FrameWindow name ="" file="" >
	<Properties>		
		<Font>Default</Font>
		<Visible>true</Visible>
		<Formatting>Centred</Formatting>
		<Title />		
		<Caption>
			<Left Image="TitlebarLeft" />
			<Right Image="TitlebarRight" />
			<Background Image="TitlebarMiddle" />
		</Caption>
		<FrameImagery>
			<Background Image="Background" />
			<Top Image="" />
			<TopLeft Image="" />
			<TopRight Image="" />
			<Left Image="FrameLeft" />
			<Right Image="FrameRight" />
			<BottomLeft Image="FrameBottomLeft" />
			<BottomRight Image="FrameBottomRight" />
			<Bottom Image="FrameBottom" />
		</FrameImagery>		
	</Properties>
	<Children>
        <ImageButton name="Close" file="base\closebutton.xml">
            <Properties>
            	<Stick>top | right</Stick>
				<StickRect>0 1 1 0</StickRect>
                <Align>top | right</Align>
            </Properties>
        </ImageButton>
        <ImageButton name="Resize" file="base\resize_button.xml">
            <Properties>
            	<Stick>bottom | right</Stick>
				<StickRect>0 1 1 0</StickRect>
				<Size>8 8</Size>
                <Align>top | right</Align>
            </Properties>
        </ImageButton>        
<!--         <ImageButton name="Lock" file="base\lockbutton.xml">
            <Properties>
                <Stick>top | left</Stick>
				<StickRect>2 2 0 0</StickRect>
                <Align>top | left</Align>
            </Properties>
        </ImageButton> -->
    </Children>
</FrameWindow>
