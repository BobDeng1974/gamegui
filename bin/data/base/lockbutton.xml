<?xml version="1.0" ?>
<ImageButton name ="" file="" >
	<Properties>
		<Visible>true</Visible>
		<TabStop>false</TabStop>
        <Size>13 11</Size>
		<States>
			<Normal image="UnlockNormal"/>
			<Hovered image="UnlockHovered"/>
			<Pushed image="UnlockPushed"/>
		</States>
	</Properties>
	<Events>
        <On_Clicked>
            local fw = to_framewindow(this.parent)
            fw.movable = not fw.movable
        </On_Clicked>
    </Events>
</ImageButton>