<?xml version="1.0" ?>
<Menu name="Menu" file="">
    <Properties>
        <Font>Default</Font>
        <FrameImagery imageset="skin">
            <Background Image="WindowFill" />
            <Top Image="WindowBorderTop" />
            <TopLeft Image="WindowBorderTopLeft" />
            <TopRight Image="WindowBorderTopRight" />
            <Left Image="WindowBorderLeft" />
            <Right Image="WindowBorderRight" />
            <BottomLeft Image="WindowBorderBottomLeft" />
            <BottomRight Image="WindowBorderBottomRight" />
            <Bottom Image="WindowBorderBottom" />
        </FrameImagery>
        <SelectImagery imageset="skin" Image="TextSelectionBrush" />
        <Items>
            <Item text="File" icon="skin:FileMenuItem" color="0.7 0.7 0.7 1" selcolor="1 1 1 1"/>
        </Items>

    </Properties>
    <Events>
		  <On_MenuClicked>
			  --ContextMenu.On_MenuClicked()
        log:msg('On Menu Load');
		  </On_MenuClicked>
      <On_Load>
        --ContextMenu.On_MenuClicked()
        log:msg('On Menu Load');
      </On_Load>
    </Events>
</Menu>
