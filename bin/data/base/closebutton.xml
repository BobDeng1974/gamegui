<?xml version="1.0" ?>
<ImageButton name ="" file="" > 
	<Properties>
		<Visible>true</Visible>
		<TabStop>false</TabStop> 
    	<Size>17 17</Size>
    	<Spacing>1.0</Spacing>
    	<Font>Default</Font>
    	<Formatting>Centred</Formatting>    
		<States>
			<Normal image="CloseButtonHover" />
			<Hovered image="CloseButton" />
			<Pushed image="CloseButtonPushed" />
		</States>
	</Properties>
	<Events>
        <On_Clicked>this.parent.visible = false</On_Clicked>
    </Events>
</ImageButton >
