<?xml version="1.0" ?>
<FrameWindow name ="Base_Island_Background" file="" >
	<Properties>
		<Visible>true</Visible>
		<Area>0 0 1024 768</Area>
	</Properties>
	<Children>
        <ImageBox name="Map_Italy" file="" >
          <Properties>
            <Area>0 0 1024 768</Area>
            <Imageset>skin_interface_main</Imageset>
            <Image>Map_Italy</Image>
            <Visible>true</Visible>
            <VertFormatting>Stretch</VertFormatting>
            <HorzFormatting>Stretch</HorzFormatting>
          </Properties>
          <Children />
          <Events />
        </ImageBox>
    </Children>
	<Events>
        <On_Clicked>
            local fw = to_framewindow(this.parent)
            fw.movable = not fw.movable
        </On_Clicked>
    </Events>
</FrameWindow>